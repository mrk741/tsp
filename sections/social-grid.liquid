            }
          {% endif %}
          {% if section.settings.desktop_height_type == 'adapt' %}
            #shopify-section-{{ section.id }} .banner__wrapper {
              aspect-ratio: calc({% if section.settings.first_image != blank %}{{ section.settings.first_image.aspect_ratio }}{% elsif section.settings.first_image == blank and section.settings.second_image != blank %}{{ section.settings.second_image.aspect_ratio }}{% elsif section.settings.mobile_image != blank %}{{ section.settings.mobile_image.aspect_ratio }}{% else %}16 / 9{% endif %}{% if section.settings.first_image != blank and section.settings.second_image != blank %} * 2{% endif %});
