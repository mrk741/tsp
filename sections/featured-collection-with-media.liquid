{%- if section.settings.video_load != blank or section.settings.image != blank or section.blocks.size > 0 or section.settings.collection != blank or request.design_mode -%}
    {{ 'section-media-with-text.css' | asset_url | stylesheet_tag }}
    {{ 'featured-collection-with-media.css' | asset_url | stylesheet_tag }}

    {%- liquid
        assign disable_max_width = false
        if section.settings.disable_max_width 
          assign disable_max_width = true
        elsif section.settings.grid_style == 'page' and settings.full_width_page_grid
          assign disable_max_width = true
        elsif section.settings.grid_style == 'alternative' and settings.full_width_alt_grid
          assign disable_max_width = true
        elsif section.settings.grid_style == 'secondary' and settings.full_width_second_grid
          assign disable_max_width = true
        endif
    
        assign max_width = '100vw'
        if disable_max_width == false and section.settings.grid_style == 'page'
            assign max_width = settings.page_width | append: 'px'
        elsif disable_max_width == false and section.settings.grid_style == 'alternative'
            assign max_width = settings.alt_grid_width | append: 'px'
        elsif disable_max_width == false and section.settings.grid_style == 'secondary'
            assign max_width = settings.second_grid_width | append: 'px'
        endif

        assign full_width = false
        if section.settings.full_width
          assign max_width = '100vw'
          assign full_width = true
        endif
    
        assign section_framing = false
        if section.settings.section_framing != 'disable'
            assign section_framing = true
        endif
    
        assign inside_link = false
        if section.settings.link != blank
            assign inside_link = true
        endif
    
        if section.settings.media_size == 'large'
          assign media_size = 0.65
        elsif section.settings.media_size == 'half'
          assign media_size = 0.50
        elsif section.settings.media_size == 'small'
          assign media_size = 0.35
        endif

        assign desktop_item_width = section.settings.desktop_item_width | times: 1
        assign desktop_item_size = desktop_item_width | floor

        assign mobile_item_size = 100.00 | divided_by: section.settings.mobile_item_min_width

        assign accent_price = false
        if settings.bolder_product_price and section.settings.card_content_style == 'regular'
            assign accent_price = true
        endif
        if settings.small_bolder_product_price and section.settings.card_content_style == 'small'
            assign accent_price = true
        endif
        assign use_accent_color_for_price = false
        if settings.use_accent_color_for_price and section.settings.card_content_style == 'regular'
            assign use_accent_color_for_price = true
        elsif settings.small_use_accent_color_for_price and section.settings.card_content_style == 'small'
            assign use_accent_color_for_price = true
        endif
        assign compare_at_price_color = 'sale'
        if section.settings.card_content_style == 'regular'
          assign compare_at_price_color = settings.compare_at_price_color
        elsif section.settings.card_content_style == 'small'
          assign compare_at_price_color = settings.small_compare_at_price_color
        endif

        if section.settings.desktop_columns == '3'
            if section.settings.collection.products.count > 2
                assign columns = section.settings.desktop_columns
            else
                assign columns = '2'
            endif
        elsif section.settings.desktop_columns == '4'
            if section.settings.collection.products.count > 3
                assign columns = section.settings.desktop_columns
            elsif section.settings.collection.products.count == 3
                assign columns = '3'
            else 
                assign columns = '2'
            endif
        elsif section.settings.desktop_columns == '5'
            if section.settings.collection.products.count > 4
                assign columns = section.settings.desktop_columns
            elsif section.settings.collection.products.count == 4
                assign columns = '4'
            elsif section.settings.collection.products.count == 3
                assign columns = '3'
            else 
                assign columns = '2'
            endif
        else 
        assign columns = section.settings.desktop_columns
        endif

        if section.settings.collection.products.count > section.settings.products_to_show
          assign products_count = section.settings.products_to_show
        else 
          assign products_count = section.settings.collection.products.count
        endif

        if section.settings.collection == blank
        assign columns = section.settings.desktop_columns
          if section.settings.products_to_show > 10
            assign products_count = 10
          else
            assign products_count = section.settings.products_to_show
          endif
        endif

        assign selector_container = '#shopify-section-' | append: section.id
        assign without_radious = false
        assign scheme_1 = settings.color_schemes | first
        if section.settings.section_framing != 'disable'
            if section.settings.color_scheme != scheme_1 and section.settings.product_grid_color_scheme != scheme_1
                assign without_radious = true
            endif
        endif 
        if full_width
          assign without_radious = true
        endif
        assign media_with_text_padding = false
        if section.settings.color_scheme == scheme_1 and section.settings.product_grid_color_scheme != scheme_1
            assign media_with_text_padding = true
        elsif section.settings.color_scheme == scheme_1 and section.settings.product_grid_color_scheme == scheme_1
            assign media_with_text_padding = true
        endif

        assign section_img = false
        if section.settings.image != blank and section.settings.video_load == blank
            assign section_img = true
        endif
        assign widths = '100, 150, 200, 300, 400, 500, 750, 1000, 1250, 1500, 1750, 2000, 2250, 2500, 2750, 3000'
        assign desktop_img_class = "media-with-text__image media-with-text__image--desktop"
        assign mobile_img_class = "media-with-text__image media-with-text__image--mobile"
        assign fetchpriority = 'high'
        if section.index > 1 
          assign fetchpriority = 'auto'
        endif

        assign info_card_visible = false
        if section.settings.info_card_heading != blank or section.settings.info_card_subheading != blank or section.settings.info_card_text != blank or section.settings.info_card_link != blank
          assign info_card_visible = true
        endif
        if info_card_visible and section.settings.info_card_style != 'hide'
          assign products_count = products_count | plus: 1
        endif

        assign onload_script = ''
        assign loading_images = ''
        if settings.images_loading != 'disable'
            assign onload_script = 'this.parentNode.classList.add("lazyloaded");' 
            assign loading_images = 'lazy'
        elsif section.index > 2
          assign loading_images = 'lazy'
        endif
    -%}
      
    {% style %}
        #shopify-section-{{ section.id }} {
            --grid-columns: {% if section.settings.media_size == 'half' %}2{% else %}3{% endif %};
            --top-spacing: {{ section.settings.top_spacing }};
            --bottom-spacing: {{ section.settings.bottom_spacing }};
            --aspect-ratio: {{ section.settings.media_ratio }};
            --focal-point: {{ section.settings.focal_point }};
            --vertical-position: {{ section.settings.vertical_text_alignment }};
            --horizontal-position: {% if section.settings.horizontal_text_alignment == 'left' %}flex-start{% elsif section.settings.horizontal_text_alignment == 'center' %}center{% else %}flex-end{% endif %};
            --section-media-radius: {% if section_framing and section.settings.section_framing == 'grid' %}'0px'{% else %}{{ settings.images_and_sections_radius }}{% endif %};
            --video-controls-position: {{ section.settings.control_horizontal_position }};
            --sticky-offset: {{ section.settings.sticky_offset }}px;
            --variant-swatches-radius: {{ settings.variant_swatches_radius }};
            --variant-thumbnail-aspect-ratio: {{ settings.variant_thumbnail_image_ratio }};
            --variant-thumbnail-focal-point: {{ settings.variant_thumbnail_focal_point }};
            --variant-thumbnail-radius: {{ settings.variant_thumbnail_radius }};
            {% if section.settings.grid_type == 'slider' %}
                --reduce-items: 1;
                {% if section.settings.grid_style == 'page' %}
                --desktop-item-width: calc((100% - ({{ settings.page_column_gap }}px * ({{ desktop_item_size }} - var(--reduce-items)))) / ({{ desktop_item_width }} - (var(--reduce-items) - 1)));
                {% elsif section.settings.grid_style == 'alternative' %}
                --desktop-item-width: calc((100% - ({{ settings.alt_grid_column_gap }}px * ({{ desktop_item_size }} - var(--reduce-items)))) / ({{ desktop_item_width }} - (var(--reduce-items) - 1)));
                {% elsif section.settings.grid_style == 'secondary' %}
                --desktop-item-width: calc((100% - ({{ settings.second_grid_column_gap }}px * ({{ desktop_item_size }} - var(--reduce-items)))) / ({{ desktop_item_width }} - (var(--reduce-items) - 1)));
                {% endif %}
                {% if section.settings.disable_grid_gaps %}
                --desktop-item-width: calc(100% / ({{ desktop_item_width }} - (var(--reduce-items) - 1)));
                {% endif %}
                {%- if section.settings.info_card_button_radius == 'default' -%}
                  --info-button-border-radius: {{ settings.buttons_radius }};
                {%- else -%}
                  --info-button-border-radius: {{ section.settings.info_card_button_radius }};
                {%- endif -%}
            {% endif %}
        }
        #shopify-section-{{ section.id }} .featured-collection__products {
            --aspect-ratio: {{ settings.product_media_ratio }};
            --focal-point: {{ settings.product_focal_point }};
        }
        {% if settings.images_loading != 'disable' and section.settings.media_ratio == 'auto' %}
          #shopify-section-{{ section.id }} .media-with-text__media-wrapper .lazy-image,
          #shopify-section-{{ section.id }} .media-with-text__media-wrapper .lazy-video {
            aspect-ratio: var(--asp-rat);
          }
        {% endif %}
        #product-grid--{{ section.id }}.page-grid-gaps {
          grid-template-columns: repeat(var(--grid-columns), calc((100% - (var(--page-column-gap) * (var(--grid-columns) - 1))) / var(--grid-columns)));
        }
        {% if section.settings.disable_grid_gaps and section.settings.grid_style == 'page' %}
          #product-grid--{{ section.id }}:not(.page-grid-gaps) {
              grid-template-columns: repeat(var(--grid-columns), calc(100% / var(--grid-columns)));
          }
        {% endif %}
        #product-grid--{{ section.id }}.alternative-grid-gaps {
            grid-template-columns: repeat(var(--grid-columns), calc((100% - (var(--alt-column-gap) * (var(--grid-columns) - 1))) / var(--grid-columns)));
        }
        {% if section.settings.disable_grid_gaps and section.settings.grid_style == 'alternative' %}
          #product-grid--{{ section.id }}:not(.alternative-grid-gaps) {
              grid-template-columns: repeat(var(--grid-columns), calc(100% / var(--grid-columns)));
          }
        {% endif %}
        #product-grid--{{ section.id }}.secondary-grid-gaps {
            grid-template-columns: repeat(var(--grid-columns), calc((100% - (var(--second-column-gap) * (var(--grid-columns) - 1))) / var(--grid-columns)));
        }
        {% if section.settings.disable_grid_gaps and section.settings.grid_style == 'secondary' %}
          #product-grid--{{ section.id }}:not(.secondary-grid-gaps) {
              grid-template-columns: repeat(var(--grid-columns), calc(100% / var(--grid-columns)));
          }
        {% endif %}
        @media screen and (max-width: 1300px) {
            #shopify-section-{{ section.id }} {
            {% if desktop_item_size == 6 %}
                --reduce-items: 2;
            {% endif %}
            }
            {% if desktop_item_size == 6 and products_count >= 5 %}
            #shopify-section-{{ section.id }} .slider__grid--center {
                justify-content: flex-start;
            }
            {% endif %}
        }
        @media screen and (max-width: 1100px) {
            #shopify-section-{{ section.id }} {
            {% if desktop_item_size == 6 %}
                --reduce-items: 3;
            {% elsif desktop_item_size == 5 %}
                --reduce-items: 2;
            {% endif %}
            }
            {% if desktop_item_size == 6 and products_count >= 4 %}
            #shopify-section-{{ section.id }} .slider__grid--center {
                justify-content: flex-start;
            }
            {% endif %}
            {% if desktop_item_size == 5 and products_count >= 4 %}
            #shopify-section-{{ section.id }} .slider__grid--center {
                justify-content: flex-start;
            }
            {% endif %}
        }
        @media screen and (max-width: 1024px) {
            #shopify-section-{{ section.id }} {
            --desktop-item-width: {{ section.settings.mobile_item_min_width }}px;
            }
            #shopify-section-{{ section.id }} .slider__grid--center {
            justify-content: flex-start;
            }
        }
        @media screen and (max-width: 768px) {
            #shopify-section-{{ section.id }} {
                --grid-columns: 1;
            }
        }
        @media screen and (max-width: 768px) {
            #shopify-section-{{ section.id }} {
                --aspect-ratio: {{ section.settings.mobile_media_ratio }};
            }
            {% if settings.images_loading != 'disable' and section.settings.mobile_media_ratio == 'auto' %}
              #shopify-section-{{ section.id }} .media-with-text__media-wrapper .lazy-image,
              #shopify-section-{{ section.id }} .media-with-text__media-wrapper .lazy-video {
                aspect-ratio: var(--asp-rat);
              }
            {% endif %}
        }
    {% endstyle %}
  
    <div class="section {% if section.settings.show_bottom_line %} bottom-line{% endif %}{% if full_width %} section--full-width{% endif %}">
        <div class="section-container featured-collection-with-media {% if section.settings.section_framing != 'disable' and full_width == false %}featured-collection-with-media--framing{% endif %} media-with-text {{ section.settings.grid_style }}-grid-gaps">
            {% if section.settings.media != 'hide' or section.blocks.size > 0 %}
              <div class="{% unless full_width %}side-margins{% endunless %} featured-collection-with-media__item {% if section.settings.color_scheme != scheme_1 and section.settings.section_framing != 'grid' %}featured-collection-with-media__item-colored{% endif %} {% if media_with_text_padding %}featured-collection-with-media__item-bottom-margin{% endif %} {% if without_radious %}section-radius-not-all{% endif %}{% if section_framing == false or section.settings.full_width %} color-{{ section.settings.color_scheme }}{% endif %} hide-if-empty">
                  <div class="{% unless full_width %}{% if section_framing and section.settings.section_framing != 'grid' %}section-radius color-{{ section.settings.color_scheme }} elem-padding-l {% endif %}{% endunless %} {% if section.settings.color_scheme == section.settings.product_grid_color_scheme and section.settings.color_scheme != scheme_1 %}without-padding{% endif %}">
                      <div class="featured-collection__media {% if section.settings.swap_media_position %} media-with-text__media--left-media{% else %} media-with-text__media--right-media{% endif %} media-with-text__card{% if full_width %} {{ section.settings.grid_style }}-grid-gaps{% endif %}{% unless full_width %}{% if section_framing and section.settings.section_framing == 'grid' %} fill-section-inside color-{{ section.settings.color_scheme }} {{ section.settings.grid_style }}-grid{% if disable_max_width %} disable-max-width{% endif %}{% else %} {{ section.settings.grid_style }}-grid-gaps{% endif %}{% endunless %} grid
                        {% unless full_width %}{% if section_framing and section.settings.section_framing == 'enable' %} {{ section.settings.grid_style }}-grid{% if disable_max_width %} disable-max-width{% endif %}{% endif %}{% endunless %}{% if section.settings.swap_media_position %} reverse-order{% endif %} {% unless full_width %}{% if section_framing and section.settings.section_framing == 'grid' %} {{ section.settings.grid_style }}-grid{% if disable_max_width %} disable-max-width{% endif %}{% endif %}{% endunless %}{% if section_framing == false and section.settings.full_width == false %} {{ section.settings.grid_style }}-grid{% if disable_max_width %} disable-max-width{% endif %}{% endif %}">
                          <div class="media-with-text__content{% if section.settings.media_size == 'small' %} column-merge{% endif %}{% unless full_width %}{% if section_framing and section.settings.section_framing == 'grid' %}{% if section.settings.media_size == 'large' %} elem-padding-m{% else %} elem-padding-l{% endif %}{% endif %}{% endunless %}{% if section.settings.swap_media_position %} left-media{% else %} right-media{% endif %}{% if full_width %} media-with-text__content--full-width {{ section.settings.media_size }}{% if section.settings.section_framing == 'grid' %} section-framing-grid{% endif %}{% endif %}">
                            <div class="media-with-text__content-wrapper {{ section.settings.content_width }} narrow-width-{{ section.settings.horizontal_text_alignment }} {{ section.settings.horizontal_text_alignment }}{% if section.settings.enable_sticky_content and full_width or section_framing == false or section.settings.section_framing != 'grid' and section_framing %} sticky-content {% if section.settings.vertical_text_alignment == 'start' %}sticky-content--top{% elsif section.settings.vertical_text_alignment == 'center' %}sticky-content--center{% else %}sticky-content--bottom{% endif %}{% endif %}">
                                {%- for block in section.blocks -%}
                                    {%- case block.type -%}
                                        {%- when 'heading' -%}
                                            {%- if block.settings.heading != blank -%}
                                            {%- liquid
                                                assign uppercase_heading = false
                                                if block.settings.heading_style == 'caption-font' and settings.uppercase_caption
                                                    assign uppercase_heading = true
                                                elsif block.settings.heading_style == 'body-font' and settings.uppercase_body
                                                    assign uppercase_heading = true
                                                elsif block.settings.heading_style == 'subheading-font' and settings.uppercase_subheadings
                                                    assign uppercase_heading = true
                                                elsif block.settings.heading_style == 'secondary-heading' and settings.uppercase_secondary_headings
                                                    assign uppercase_heading = true
                                                elsif block.settings.heading_style == 'section-heading' and settings.uppercase_section_headings
                                                    assign uppercase_heading = true
                                                elsif block.settings.heading_style == 'page-title' and settings.uppercase_page_titles
                                                    assign uppercase_heading = true
                                                endif
  
                                              assign heading_text = block.settings.heading
                                              assign processed_text = ''
                                              assign split_text = heading_text | split: '['

                                              for part in split_text
                                                if part contains ']'
                                                  assign split_bracket = part | split: ']'
                                                  assign highlight_text = split_bracket[0]
                                                  assign after_bracket = split_bracket[1]
                                                  assign processed_text = processed_text | append: '<span class="highlight"><span>' | append: highlight_text | append: '</span></span>' | append: after_bracket
                                                else
                                                  assign processed_text = processed_text | append: part
                                                endif
                                              endfor

                                              
                                              assign final_text = ''
                                              assign split_text_curly = processed_text | split: '{'

                                              for part in split_text_curly
                                                if part contains '}'
                                                  assign split_curly = part | split: '}'
                                                  assign styled_text = split_curly[0]
                                                  assign after_curly = split_curly[1]
                                                  assign final_text = final_text | append: '<span class="styled">' | append: styled_text | append: '</span>' | append: after_curly
                                                else
                                                  assign final_text = final_text | append: part
                                                endif
                                              endfor
                                          -%}
                                            <{{ block.settings.heading_tag }} class="section-block custom-heading heading-block {{ block.settings.heading_style }}{% if uppercase_heading %} uppercase{% endif %}{% if block.settings.heading_style == 'subheading-font' and settings.use_heading_font_subheadings %} subheading-heading-font{% endif %}" {{ block.shopify_attributes }}>
                                              {% if block.settings.heading_style == 'subheading-font' and settings.bolder_subheadings_font %}<b>{% endif %}{{ final_text }}{% if block.settings.heading_style == 'subheading-font' and settings.bolder_subheadings_font %}</b>{% endif %}
                                            </{{ block.settings.heading_tag }}>
                                            {%- endif -%}
                                        {%- when 'subheading' -%}
                                            {%- if block.settings.subheading != blank -%}
                                            {%- liquid
                                                assign uppercase_subheading = false
                                                if block.settings.subheading_style == 'section-heading' and settings.uppercase_section_headings
                                                  assign uppercase_subheading = true
                                                elsif block.settings.subheading_style == 'secondary-heading' and settings.uppercase_secondary_headings
                                                  assign uppercase_subheading = true
                                                elsif block.settings.subheading_style == 'subheading-font' and settings.uppercase_subheadings
                                                    assign uppercase_subheading = true
                                                elsif block.settings.subheading_style == 'body-font' and settings.uppercase_body
                                                    assign uppercase_subheading = true
                                                elsif block.settings.subheading_style == 'caption-font' and settings.uppercase_caption
                                                    assign uppercase_subheading = true
                                                endif
                                            -%}
                                            <{{ block.settings.subheading_tag }} class="section-block subheading-block {{ block.settings.subheading_style }}{% if uppercase_subheading %} uppercase{% endif %}{% if block.settings.subheading_style == 'subheading-font' and settings.use_heading_font_subheadings %} subheading-heading-font{% endif %}" {{ block.shopify_attributes }}>
                                                {% if block.settings.subheading_style == 'subheading-font' and settings.bolder_subheadings_font %}<b>{% endif %}{{ block.settings.subheading }}{% if block.settings.subheading_style == 'subheading-font' and settings.bolder_subheadings_font %}</b>{% endif %}
                                            </{{ block.settings.subheading_tag }}>
                                            {%- endif -%}
                                        {%- when 'text' -%}
                                            {%- if block.settings.text != blank -%}
                                            {%- liquid
                                                assign uppercase_text = false
                                                if block.settings.text_style == 'section-heading' and settings.uppercase_section_headings
                                                  assign uppercase_text = true
                                                elsif block.settings.text_style == 'secondary-heading' and settings.uppercase_secondary_headings
                                                  assign uppercase_text = true
                                                elsif block.settings.text_style == 'subheading-font' and settings.uppercase_subheadings
                                                    assign uppercase_text = true
                                                elsif block.settings.text_style == 'body-font' and settings.uppercase_body
                                                    assign uppercase_text = true
                                                elsif block.settings.text_style == 'caption-font' and settings.uppercase_caption
                                                    assign uppercase_text = true
                                                endif
                                            -%}
                                            <div class="section-block richtext rte link-underline {{ block.settings.text_style }}{% if uppercase_text %} uppercase{% endif %} 
                                            {% if block.settings.text_style == 'subheading-font' %}{% if settings.use_heading_font_subheadings %} subheading-heading-font{% endif %}{% if settings.bolder_subheadings_font %} bold{% endif %}{% endif %}" {{ block.shopify_attributes }}>
                                                {{ block.settings.text }}
                                            </div>
                                            {%- endif -%}
                                        {%- when 'custom_liquid' -%}
                                            {%- if block.settings.custom_liquid != blank -%}
                                            {%- liquid
                                                assign uppercase_text = false
                                                if block.settings.text_style == 'section-heading' and settings.uppercase_section_headings
                                                  assign uppercase_text = true
                                                elsif block.settings.text_style == 'secondary-heading' and settings.uppercase_secondary_headings
                                                assign uppercase_text = true
                                                elsif block.settings.text_style == 'subheading-font' and settings.uppercase_subheadings
                                                    assign uppercase_text = true
                                                elsif block.settings.text_style == 'body-font' and settings.uppercase_body
                                                    assign uppercase_text = true
                                                elsif block.settings.text_style == 'caption-font' and settings.uppercase_caption
                                                    assign uppercase_text = true
                                                endif
                                            -%}
                                            <div class="custom-liquid {{ block.settings.text_style }}{% if uppercase_text %} uppercase{% endif %} 
                                                {% if block.settings.text_style == 'subheading-font' %}{% if settings.use_heading_font_subheadings %} subheading-heading-font{% endif %}{% if settings.bolder_subheadings_font %} bold{% endif %}{% endif %}" {{ block.shopify_attributes }}>
                                                {{ block.settings.custom_liquid }}
                                            </div>
                                            {%- endif -%}
                                        {%- when 'context_image' -%}
                                            {%- liquid
                                                render 'context-image', location: block, block_shopify_attributes: true, section_block: true, inside_link: inside_link -%}
                                        {%- when 'icon' -%}
                                            {%- liquid
                                              render 'block-icon', block: block -%}
                                        {%- when 'button' -%}
                                            {%- liquid
                                                render 'button', block: block, inside_link: inside_link -%}
                                        {%- when 'video_button' -%}
                                            {%- render 'video-button', block: block -%}
                                        {%- when 'empty_space' -%}
                                            {%- render 'empty-space', block: block -%}
                                        {%- when 'line' -%}
                                            {%- render 'line', block: block -%}
                                    {%- endcase -%}
                                {%- endfor -%}
                            </div>
                          </div>
                          {% if section.settings.media != 'hide' %}
                              <div class="media-with-text__media media-with-text__media--{{ section.settings.media }} {% if section.settings.media_size == 'large' %} column-merge{% endif %}{% if section.settings.enable_mute or section.settings.enable_pause %} video-controls-js{% endif %}">
                                  {%- if section.settings.video_load != blank -%}
                                    {% if section.settings.enable_mute or section.settings.enable_pause %}
                                      <video-controls class="video-controls elem-padding-s">
                                        <div class="video-controls__wrapper flex--row">
                                          {% if section.settings.enable_pause %}
                                            <button class="button--controls icon icon--large button--pause play" aria-label="{{ 'accessibility.pause' | t }}">
                                              <span class="icon--pause">{%- render 'glyphs_interface', icon: 'pause-circle' -%}</span>
                                              <span class="icon--play">{%- render 'glyphs_interface', icon: 'play-circle' -%}</span>
                                            </button>
                                          {% endif %}
                                          {% if section.settings.enable_mute %}
                                            <button class="button--controls icon icon--large button--mute mute" aria-label="{{ 'accessibility.unmute' | t }}">
