                        assign bold_price = false
                        if block.settings.use_main_button and settings.bolder_main_buttons_font
                          assign bold_price = true
                        endif
                        if block.settings.use_main_button == false and settings.bolder_secondary_buttons_font
                          assign bold_price = true
                        endif
                      %}
