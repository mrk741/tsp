                              <input 
                                  id="{{ form_id }}-name" 
                                  class="input input--text required {% if form.errors %}invalid{% endif %}" 
                                  autocomplete="name" 
                                  type="text" 
                                  name="contact[{{ 'templates.contact.form.name' | t }}]" 
                                  value="{% if form.name %}{{ form.name }}{% elsif customer %}{{ customer.name }}{% endif %}" 
                                  placeholder="{{ block.settings.name_placeholder }}"
                                  {% if block.settings.make_name_required %}required{% endif %}/>
                          </div>
                        {% endif %}
                        <div class="field">
                            <label class="" for="{{ form_id }}-email">{{ 'templates.contact.form.email' | t | append: '*' }}</label>
                            <input
                                id="{{ form_id }}-email"
                                autocomplete="email"
                                type="email"
                                dir="ltr"
                                id="ContactForm-email-{{ section.id }}"
                                class="input input--text required {% if form.errors contains 'email' %}invalid{% endif %}"
                                name="contact[email]"
                                spellcheck="false"
                                autocapitalize="off"
                                value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}"
                                aria-required="true"
                                {% if form.errors contains 'email' %}
                                aria-invalid="true"
                                aria-describedby="ContactForm-email-error-{{ section.id }}"
                                {% endif %}
                                placeholder="{{ block.settings.email_placeholder }}"
                                required="required"/>
                        </div>
                        {%- if block.settings.show_phone -%}
                        <div class="field">
                            <label class="label" for="{{ form_id }}-phone">{{ 'templates.contact.form.phone' | t }}</label>
                            <input 
                                id="{{ form_id }}-phone" 
                                type="tel" 
                                dir="ltr"
                                class="input input--text" 
                                autocomplete="tel" 
                                name="contact[{{ 'templates.contact.form.phone' | t }}]" 
                                pattern="[0-9\-]*" 
