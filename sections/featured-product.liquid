                        </div>
                      </div>
                    {% endif %}
                    {% if product != blank %}
                      {% if settings.disable_shopping == false  %}
                        {% render 'buy-buttons', block: block, product: product, product_form_id: product_form_id, section_id: section.id %}
                      {% endif %}
                    {% else %}
                    {% if settings.disable_shopping == false  %}
                    <div class="buy-buttons" style="--spacing: {{ block.settings.top_spacing }};--mobile-spacing: {{ block.settings.mobile_top_spacing }};" {{ block.shopify_attributes }}>
                      <div class="product-form__buttons flex--column">
                        {% if block.settings.style == 'show_quantity' %}<div class="product-form__submit-wrapper flex--row">
                          <div
                            id="Quantity-Form-{{ section.id }}"
                            class="product-form__input flex--column product-form__quantity{% if settings.inputs_shadow_vertical_offset != 0 and settings.inputs_shadow_vertical_offset < 0 %} product-form__quantity-top{% endif %} 
                              body-font {% if settings.uppercase_body %} uppercase{% endif %}">
                            <label class="visually-hidden" for="Quantity-{{ section.id }}">{{ 'products.product.quantity.input_label' | t | escape }}</label>
                            <quantity-input class="quantity flex--row" data-section="{{ section.id }}">
                              <button class="quantity__button icon--large" name="minus" type="button">
                                <span class="visually-hidden">
