                      <span class="line"></span>
                    </div>
                  {% endif %}
                {%- when 'complementary_products' -%}
                  {% if second_container_index > forloop.index or second_container == false %}{%- render 'complementary-products', block: block, selector_container: selector_container, product: product -%}{% endif %}
                {% when 'linked_products' %}
                  {% if second_container_index > forloop.index or second_container == false %}{% render 'linked-products', product: product, block: block %}{% endif %}
                {% when 'tabs' %}
                  {% if second_container_index > forloop.index or second_container == false %}{%- render 'tabs', block: block, product_block: true, product: product -%}{% endif %}
                {%- when 'text' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- if block.settings.text != blank -%}
                      {%- liquid
                        assign uppercase_headings = false
                        if block.settings.richtext_heading_style == 'body-font' and settings.uppercase_body
                          assign uppercase_headings = true
                        elsif block.settings.richtext_heading_style == 'subheading-font' and settings.uppercase_subheadings
                          assign uppercase_headings = true
                        elsif block.settings.richtext_heading_style == 'secondary-heading' and settings.uppercase_secondary_headings
                          assign uppercase_headings = true
                        elsif block.settings.richtext_heading_style == 'section-heading' and settings.uppercase_section_headings
                            assign uppercase_headings = true
                        elsif block.settings.richtext_heading_style == 'page-title' and settings.uppercase_page_titles
                            assign uppercase_headings = true
                        endif
                      -%}
                      <div class="section-block{% if block.settings.color_scheme != section.settings.color_scheme %} color-{{ block.settings.color_scheme }} elem-padding-s section-radius{% endif %} richtext heading-style-{{ block.settings.richtext_heading_style }} rte link-underline{% if uppercase_headings %} headings-uppercase{% endif %}
                        {% if block.settings.richtext_heading_style == 'subheading-font' %}{% if settings.use_heading_font_subheadings %} heading-style-subheading-heading-font{% endif %}{% if settings.bolder_subheadings_font %} heading-style-bold{% endif %}{% endif %} {{ block.settings.text_style }}
                        {% if block.settings.text_style == 'subheading-font' and settings.use_heading_font_subheadings %} subheading-heading-font{% endif %}" style="--spacing: {{ block.settings.top_spacing }};--mobile-spacing: {{ block.settings.mobile_top_spacing }};" {{ block.shopify_attributes }}>
                            {{ block.settings.text }}
                      </div>
                    {%- endif -%}
                  {% endif %}
                {%- when 'empty_space' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- render 'empty-space', block: block -%}
                  {% endif %}
                {%- when 'more_details' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {% if block.settings.button_label != blank or block.settings.button_icon != blank %}
                      <div class="more-details{% if block.settings.button_label != blank and block.settings.button_icon != blank %} button-block--small-padding{% endif %}" style="--spacing: {{ block.settings.top_spacing }};--mobile-spacing: {{ block.settings.mobile_top_spacing }};" {{ block.shopify_attributes }}>
                          <a class="more-details__link icon-button button link-button button--secondary {{ block.settings.text_style }}{% if block.settings.uppercase %} uppercase{% endif %}{% if block.settings.text_style == 'subheading-font' and settings.use_heading_font_subheadings %} subheading-heading-font{% endif %}{% if block.settings.button_icon != blank %} button-with-icon{% if block.settings.swap_button_icon %} icon--swap{% endif %}{% endif %}" href="{{ product.url }}">
                            {% if block.settings.button_icon != blank %}
                              {%- liquid
                                assign icon_name = block.settings.button_icon | split: '/'
                                if icon_name.size == 3
                                    assign file_name = icon_name.first | append: '_' | append: icon_name[1]
                                elsif icon_name.size == 2
                                    assign file_name = icon_name.first
                                endif
                                assign icon = icon_name.last
                              -%}
                              <span class="icon-accordion flex--row icon--small">{% render 'catalog-icons', file_name: file_name, icon: icon %}</span>
                            {% endif %}
                            {%- if block.settings.button_label != blank -%}
                            {% if block.settings.bolder_font %}<b>{% endif %}{{ block.settings.button_label }}{% if block.settings.bolder_font %}</b>{% endif %}
                            <div class="icon icon--large">
                              {%- render 'glyphs_arrows', icon: 'arrow-right' -%}
                            </div>
                            {%- endif -%}
                          </a>
                      </div>
                    {% endif %}
                  {% endif %}
                {%- when 'ask_question' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- render 'ask-question', block: block -%}
                  {% endif %}
                {%- when 'payment_methods' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    <div class="section-block flex--row" 
                      style="--spacing: {{ block.settings.top_spacing }}; --mobile-spacing: {{ block.settings.mobile_top_spacing }}; --icon-width: {{ block.settings.icon_width }}px;" {{ block.shopify_attributes }}>
                      <div class="payment">
                          <ul class="payment__list flex--row" role="list">
                          {%- for type in shop.enabled_payment_types -%}
                              <li class="payment__list-item{% if block.settings.black_and_white %} black-and-white{% endif %}">
                              {{ type | payment_type_svg_tag }}
                              </li>
                          {%- endfor -%}
                          </ul>
                      </div>
                    </div>
                  {% endif %}
                {%- when 'custom_options_pills' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- render 'custom-options-pills', block: block -%}
                  {% endif %}
                {%- when 'custom_options_measur' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- render 'custom-options-measurement', block: block -%}
                  {% endif %}
                {%- when 'custom_options_checkbox' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- render 'custom-options-checkbox', block: block -%}
                  {% endif %}
                {%- when 'custom_options_dropdown' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- render 'custom-options-dropdown', block: block -%}
                  {% endif %}
                {%- when 'custom_options_s_line' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- render 'custom-options-single-line', block: block -%}
                  {% endif %}
                {%- when 'custom_options_m_line' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- render 'custom-options-multi-line', block: block -%}
                  {% endif %}
                {%- when 'custom_options_upload' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- render 'custom-options-upload', block: block -%}
                  {% endif %}
                {%- when 'custom_options_date' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- render 'custom-options-date', block: block -%}
                  {% endif %}
                {%- when 'custom_options_swatches' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- render 'custom-options-swatches', block: block -%}
                  {% endif %}
                {%- when 'custom_options_color' -%}
                  {% if second_container_index > forloop.index or second_container == false %}
                    {%- render 'custom-options-color', block: block -%}
                  {% endif %}
              {%- endcase -%}
            {% endfor %}
          </div>
        </product-info>
        {% if section.settings.content_layout == 'second_below_media' and second_container and section.settings.top_container_on_mobile == 'first' %}
          {% for block in section.blocks %}
            {%- case block.type -%}
            {% when 'second_container' %}
              <div class="product-details product-details--only-mobile product-details--{{ section.settings.content_layout }} product-details-mobile--{{ section.settings.top_container_on_mobile }} product__info-container">
                <div class="product-details--wrapper">
                    <div class="product-details__block {{ block.settings.heading_style }}{% if block.settings.uppercase %} uppercase{% endif %}{% if block.settings.heading_style == 'subheading-font' and settings.use_heading_font_subheadings %} subheading-heading-font{% endif %}" 
                      style="--spacing: {{ block.settings.top_spacing }};--bottom-spacing: {{ block.settings.bottom_spacing }};--mobile-spacing: {{ block.settings.mobile_top_spacing }};--mobile-bottom-spacing: {{ block.settings.mobile_bottom_spacing }};" {{ block.shopify_attributes }}>
                      <p>{% if block.settings.bolder_font %}<b>{% endif %}{{ block.settings.heading }}{% if block.settings.bolder_font %}</b>{% endif %}</p>
                    </div>
                  {% render 'product-details-blocks', section: section, block: block, second_container: second_container, second_container_index: second_container_index, product: product, selector_container: selector_container, loading_images: loading_images, onload_script: onload_script %}
                </div>
              </div>
            {%- endcase -%}
          {% endfor %}
        {% endif %}
        {% if section.settings.content_layout == 'only_first' or section.settings.content_layout == 'first_and_second_stretched' %}
          {% if second_container %}
            {% for block in section.blocks %}
              {%- case block.type -%}
              {% when 'second_container' %}
                <div class="product-details product-details--{{ section.settings.content_layout }} product-details-mobile--{{ section.settings.top_container_on_mobile }} product__info-container">
                  <div class="product-details--wrapper">
                      <div class="product-details__block {{ block.settings.heading_style }}{% if block.settings.uppercase %} uppercase{% endif %}{% if block.settings.heading_style == 'subheading-font' and settings.use_heading_font_subheadings %} subheading-heading-font{% endif %}" 
                        style="--spacing: {{ block.settings.top_spacing }};--bottom-spacing: {{ block.settings.bottom_spacing }};--mobile-spacing: {{ block.settings.mobile_top_spacing }};--mobile-bottom-spacing: {{ block.settings.mobile_bottom_spacing }};" {{ block.shopify_attributes }}>
                        <p>{% if block.settings.bolder_font %}<b>{% endif %}{{ block.settings.heading }}{% if block.settings.bolder_font %}</b>{% endif %}</p>
                      </div>
                    {% render 'product-details-blocks', section: section, block: block, second_container: second_container, second_container_index: second_container_index, product: product, selector_container: selector_container, loading_images: loading_images, onload_script: onload_script %}
                  </div>
                </div>
              {%- endcase -%}
            {% endfor %}
          {% endif %}
        {% endif %}
      </div>
      {% if section.settings.content_layout == 'two_columns' and second_container %}
        {% for block in section.blocks %}
          {%- case block.type -%}
          {% when 'second_container' %}
            <div class="product-details {% if section.settings.content_layout == 'two_columns' and second_container %}product-details--{{ section.settings.second_container_alignment }}{% endif %} flex--column product__info-container product-details--{{ section.settings.content_layout }} product-details-mobile--{{ section.settings.top_container_on_mobile }}">
                <div class="product-details--wrapper">
                  <div class="product-details--wrapper-container">
                      <div class="product-details__block {{ block.settings.heading_style }}{% if block.settings.uppercase %} uppercase{% endif %}{% if block.settings.heading_style == 'subheading-font' and settings.use_heading_font_subheadings %} subheading-heading-font{% endif %}" 
                        style="--spacing: {{ block.settings.top_spacing }};--bottom-spacing: {{ block.settings.bottom_spacing }};--mobile-spacing: {{ block.settings.mobile_top_spacing }};--mobile-bottom-spacing: {{ block.settings.mobile_bottom_spacing }};" {{ block.shopify_attributes }}>
                        <p>{% if block.settings.bolder_font %}<b>{% endif %}{{ block.settings.heading }}{% if block.settings.bolder_font %}</b>{% endif %}</p>
                      </div>
                    {% render 'product-details-blocks', section: section, block: block, second_container: second_container, second_container_index: second_container_index, product_form_id: product_form_id, product: product, selector_container: selector_container, loading_images: loading_images, onload_script: onload_script %}
                  </div>
                </div>
            </div>
          {%- endcase -%}
        {% endfor %}
      {% endif %}
    </div>
    
  </div>
</div>
{% if section.settings.image_zoom == 'lightbox' or section.settings.image_zoom == 'slideshow' %}
  <product-modal id="ProductModal-{{ section.id }}" class="product-media-modal media-modal" data-template-id="ProductModalTemplate-{{ section.id }}"></product-modal>
  <template id="ProductModalTemplate-{{ section.id }}">
    {% style %}
      #ProductModal-{{ section.id }} {
        --item-width: {{ section.settings.desktop_width }}%;
        --focal-point: {{ section.settings.zoom_focal_point }};
        --slider-gap: 8px;
        --aspect-ratio: {{ section.settings.zoom_media_ratio }};
        {% if section.settings.image_zoom == 'slideshow'  %}
          --desktop-item-width: 100%;
        {% endif %}
      }

      @media screen and (max-width: 1024px) {
          #ProductModal-{{ section.id }} {
          --item-width: {{ section.settings.mobile_width }}%;
        }
      }
    {% endstyle %}
    <div class="product-media-modal__dialog color-{{ section.settings.color_scheme }}" role="dialog" aria-modal="true" tabindex="-1">
      <button id="ModalClose-{{ section.id }}" type="button" class="product-media-modal__toggle button solid-button button-close button--small close-popup icon--large">
        {%- render 'glyphs_interface', icon: 'close' -%}
      </button>
      <div
        class="product-media-modal__content hide-scrollbar"
        role="document"
        tabindex="0"
      >
      <div class="product-media-modal__wrapper product-media-modal__wrapper--{{ section.settings.image_zoom }}">
        {% if section.settings.image_zoom == 'slideshow' %}
          <slider-component id="GalleryViewer-{{ section.id }}-zoom" class="slider">
            <div class="slider__viewport">
              <ul id="Slider-Gallery-{{ section.id }}-zoom" class="slider__grid hide-scrollbar slider-in-product-modal" role="list" style="--desktop-columns: 1; --mobile-columns: 1;"
                data-count="1"
                data-count-mobile="1">
        {% endif %}
        {% liquid 
          assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src'
          assign variant_alt = product.selected_or_first_available_variant.image.alt | downcase
          assign variant_alt = variant_alt | split: '(' | last | split: ')' | first | append: ')' | prepend: '('
          assign admin_order = false
          if section.settings.organize_images 
            assign admin_order = true
          endif
        %}
        {% if admin_order == false %}
          {%- liquid
            if product.selected_or_first_available_variant.featured_media != null
              assign media = product.selected_or_first_available_variant.featured_media
              assign featured_media_alt = product.selected_or_first_available_variant.featured_media.alt | downcase
              if product.selected_or_first_available_variant.featured_media.alt contains '(' or product.selected_or_first_available_variant.featured_media.alt contains ')' 
                assign featured_media_alt = product.selected_or_first_available_variant.featured_media.alt | downcase | split: '(' | last | split: ')' | first | prepend: '(' | append: ')'
              else
                assign featured_media_alt = product.selected_or_first_available_variant.featured_media.alt | downcase
              endif
              if section.settings.show_media_attached_to_variant
                assign current_variant_alt = true
              else
                assign current_variant_alt = false
              endif
              render 'product-media', media: media, loop: section.settings.enable_video_looping, variant_image: section.settings.show_media_attached_to_variant, current_variant_alt: true, media_alt: featured_media_alt, hide_images: section.settings.show_media_attached_to_variant, first_3d_model: first_3d_model, autoplay: section.settings.enable_video_autoplay, style: section.settings.image_zoom, product_media: product.media, first: true
            endif
          -%}
        {% endif %}
        {%- for media in product.media -%}
          {%- liquid
            if media.alt contains '(' or media.alt contains ')' 
              assign media_alt = media.alt | downcase | split: '(' | last | split: ')' | first | prepend: '(' | append: ')' 
            else 
              assign media_alt = media.alt | downcase
            endif
            
            if variant_images contains media.src
              assign variant_image = true
            else
              assign variant_image = false
            endif
            if section.settings.show_media_attached_to_variant and variant_alt contains media_alt
              assign current_variant_alt = true
            else
              assign current_variant_alt = false
            endif
            assign admin_order = false
            if section.settings.organize_images 
              assign admin_order = true
            endif
            if admin_order == false 
              unless media.id == product.selected_or_first_available_variant.featured_media.id
                render 'product-media', media: media, variant_image: variant_image, current_variant_alt: current_variant_alt, media_alt: media_alt, hide_images: section.settings.show_media_attached_to_variant, first_3d_model: first_3d_model, autoplay: section.settings.enable_video_autoplay, style: section.settings.image_zoom, for_loop: forloop, product_media: product.media
              endunless
            elsif section.settings.organize_images
              render 'product-media', media: media, variant_image: variant_image, current_variant_alt: current_variant_alt, media_alt: media_alt, hide_images: section.settings.show_media_attached_to_variant, first_3d_model: first_3d_model, autoplay: section.settings.enable_video_autoplay, style: section.settings.image_zoom, for_loop: forloop, product_media: product.media
            endif
          -%}
        {%- endfor -%}
        {% if section.settings.image_zoom == 'slideshow' %}
        </ul>
        <div class="zoom-slider-buttons-items {% unless section.settings.show_arrow_in_mobile %} hidden-on-mobile{% endunless %}
          {% if product.media.size == 1 %}hidden-mobile{% endif %}
          {% if product.media.size == 1 %}hidden-desktop{% endif %}">
          <button type="button" class="slider-button slider-button--prev" name="previous" aria-label="{{ 'general.slider.previous_slide' | t }}">
            <span class="icon icon--small">{%- render 'glyphs_arrows', icon: 'arrow-left' -%}</span>
          </button>
          <button type="button" class="slider-button slider-button--next" name="next" aria-label="{{ 'general.slider.next_slide' | t }}">
            <span class="icon icon--small">{%- render 'glyphs_arrows', icon: 'arrow-right' -%}</span>
          </button>
        </div>
      </div>
      </slider-component>
      {% endif %}
    </div>
    </div>
    </div>
  </template>
{% endif %}

