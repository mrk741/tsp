{% endstyle %}

<div class="section side-margins{% if section.settings.full_width %} disable-margins{% endif %} slideshow section-overlap--{{ section.settings.overlap }} {% if section.settings.show_bottom_line %} bottom-line{% endif %}">
  <div class="section-container {% if section.settings.section_framing != 'disable' %}section-container-framing{% endif %} video-banner banner {{ section.settings.grid_style }}-grid {% if disable_max_width %}disable-max-width{% endif %} disable-max-width-container">
    <slideshow-js data-effect="{{ section.settings.effect }}" class="flex--row {% if section.settings.section_framing != 'disable' and section.settings.full_width == false %}section-radius elem-padding-l disable-section-framing{% endif %}">
        <slider-swiper class="swiper swiper-slideshow" data-effect="{{ section.settings.effect }}" data-transition-duration="{{ section.settings.transition_duration }}" data-autoplay="{% if section.settings.enable_autoplay %}true{% else %}false{% endif %}"{% if section.settings.enable_autoplay %}data-autoplay-speed="{{ section.settings.autoplay_speed | times: 1000.00 }}"{% endif %} data-hover-autoplay="false" data-enable-navigation="true" data-pagination-type="bullets">
        <div class="swiper-wrapper">
            {%- for block in section.blocks -%}
                {%- case block.type -%}
                    {%- when 'image' -%}
                        {% if block.settings.link != blank %}<a href="{{ block.settings.link }}" {% if block.settings.target_blank %}target="_blank"{% endif %} class="logo-{{ block.settings.logo_style }} mobile-logo-{{ block.settings.mobile_logo_style }} swiper-slide {% if forloop.index == 1 %}swiper-slide-active{% endif %} color-{{ block.settings.color_scheme }} mobile-color-{{ block.settings.mobile_color_scheme }} banner__wrapper {% if section.settings.mobile_content_under_media %} mobile-content-below{% endif %}{% if section.settings.desktop_content_under_media %} desktop-content-below{% endif %} swiper-slide--{{ block.id }} slideshow__wrapper--{{ block.id }}" {{ block.shopify_attributes }}>
                        {% else %}<div class="swiper-slide {% if forloop.index == 1 %}swiper-slide-active{% endif %} color-{{ block.settings.banner_color_scheme }} mobile-color-{{ block.settings.mobile_banner_color_scheme }} {% if block.settings.banner_color_scheme != block.settings.color_scheme %}swiper-slide--padding{% endif %} banner__wrapper {% if section.settings.mobile_content_under_media %} mobile-content-below{% endif %}{% if section.settings.desktop_content_under_media %} desktop-content-below{% endif %} swiper-slide--{{ block.id }} slideshow__wrapper--{{ block.id }} logo-{{ block.settings.logo_style }} mobile-logo-{{ block.settings.mobile_logo_style }}" {{ block.shopify_attributes }}>{% endif %}
                            {% liquid 
                                assign inside_link = false
                                if block.settings.link != blank
                                    assign inside_link = true
                                endif
                            %}
                            <div class="banner__media color-{{ block.settings.banner_color_scheme }} mobile-color-{{ block.settings.mobile_banner_color_scheme }} image-banner__media"{% if settings.disable_right_click %} oncontextmenu="return false"{% endif %}>
                                {% liquid 
                                    assign widths = '300, 400, 500, 750, 1000, 1250, 1500, 1750, 2000, 2250, 2500, 2600, 2750, 3000, 3200'
                                    assign img_class = "banner__image image-banner__image"
                                    assign fetchpriority = 'high'
                                    if forloop.index > 1
                                        assign loading_images = 'lazy'
                                        assign fetchpriority = 'auto'
                                    endif
                                %}
                                {%- capture sizes -%}
                                {% if disable_max_width %}
                                {{ max_width }}
                                {% else %}
                                (max-width: 768px) 100vw, (max-width: {{ max_width }}) {{ max_width }}, {{ max_width }}
                                {% endif %}
                            {%- endcapture -%}
                            {%- if block.settings.desktop_image != blank -%}
                                <div class="image-banner__media-block slideshow__media {% if block.settings.mobile_image != blank %}slideshow__media--desktop{% endif %}"{% if settings.disable_right_click %} oncontextmenu="return false"{% endif %}>
                                {% if lazy_loading_images %}
                                    <figure class="lazy-image lazy-image--absolut lazy-image--{{ settings.images_loading }} ">
                                {% endif %}
                                {{
                                    block.settings.desktop_image
                                    | image_url: width: block.settings.desktop_image.width
                                    | image_tag:
                                    sizes: sizes,
                                    widths: widths,
                                    height: block.settings.desktop_image.height,
                                    class: img_class,
                                    alt: block.settings.desktop_image.alt,
                                    draggable: false,
                                    loading: loading_images,
                                    fetchpriority: fetchpriority,
                                    onload: onload_script
                                }}
                                {% if lazy_loading_images %}
                                    {% if settings.images_loading != 'fade_scale' %}
                                    <div class="lazy-image__preloader lazy-image__preloader-{{ settings.images_loading }}" aria-hidden="true">
                                      <img 
                                      alt="{{ block.settings.desktop_image.alt | escape }}" 
                                      src="{{ block.settings.desktop_image | image_url: width: 10 }}"
                                      draggable="false"
                                      width="10" 
                                      height="10">
                                    </div>
                                    {% endif %}
                                  </figure>
                                {% endif %}
                                </div>
                            {%- endif -%}
                            {%- if block.settings.mobile_image != blank -%}
                                <div class="image-banner__media-block slideshow__media {% if block.settings.desktop_image != blank %}slideshow__media--mobile{% endif %}"{% if settings.disable_right_click %} oncontextmenu="return false"{% endif %}>
                                {% if lazy_loading_images %}
                                    <figure class="lazy-image lazy-image--absolut lazy-image--{{ settings.images_loading }} ">
                                {% endif %}
                                {{
                                    block.settings.mobile_image
                                    | image_url: width: block.settings.mobile_image.width
                                    | image_tag:
                                    sizes: sizes,
                                    widths: widths,
                                    height: block.settings.mobile_image.height,
                                    class: img_class,
                                    alt: block.settings.mobile_image.alt,
                                    draggable: false,
                                    loading: loading_images,
                                    fetchpriority: fetchpriority,
                                    onload: onload_script
                                }}
                                {% if lazy_loading_images %}
                                    {% if settings.images_loading != 'fade_scale' %}
                                    <div class="lazy-image__preloader lazy-image__preloader-{{ settings.images_loading }}" aria-hidden="true">
                                      <img 
                                      alt="{{ block.settings.mobile_image.alt | escape }}" 
                                      src="{{ block.settings.mobile_image | image_url: width: 10 }}"
                                      draggable="false"
                                      width="10" 
                                      height="10">
                                    </div>
                                    {% endif %}
                                  </figure>
                                {% endif %}
                                </div>
                            {%- endif -%}
                            {% if block.settings.desktop_image == blank and block.settings.mobile_image == blank %}
                                <div class="image-banner__media-block slideshow__media"{% if settings.disable_right_click %} oncontextmenu="return false"{% endif %}>
                                    {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                </div>
                            {% endif %} 
                            </div>
                            <div class="banner__container-content banner__container-content--{{ block.settings.horizontal_content_position }} {{ section.settings.grid_style }}-grid disable-max-width color-{{ block.settings.banner_color_scheme }} mobile-color-{{ block.settings.mobile_banner_color_scheme }}">
                            <div class="banner__content {{ block.settings.content_width }} elem-padding-l {% if block.settings.vertical_content_position == 'end' %}add-padding-for-bullet{% endif %} color-{{ block.settings.banner_color_scheme }} mobile-color-{{ block.settings.mobile_banner_color_scheme }}">
                                <div class="banner__content-wrapper{% if block.settings.center_text %} center{% endif %}{% if block.settings.fill_content %} fill-content{% if block.settings.content_width == 'wide' %} elem-padding-l{% else %} elem-padding-m{% endif %}{% endif %}">
                                    {%- if block.settings.context_image != blank -%}
                                        {%- render 'context-image', location: block, section_block: true, inside_link: inside_link -%}
                                      {%- elsif block.settings.icon != blank -%}
                                        {%- liquid
                                          assign icon_name = block.settings.icon | split: '/'
                                          if icon_name.size == 3
                                              assign file_name = icon_name.first | append: '_' | append: icon_name[1]
                                          elsif icon_name.size == 2
                                              assign file_name = icon_name.first
                                          endif
                                          assign icon = icon_name.last
                                          assign color_rgb = block.settings.color | color_to_rgb | strip
                                          assign mobile_color_rgb = block.settings.mobile_color | color_to_rgb | strip
                                        -%}
                                        {%- if block.settings.context_image_link != blank -%}
                                            {%- if inside_link -%}
                                            <object class="section-block section-block--icon" aria-label="{{ 'general.links.link_to_button_label' | t: label: block.settings.context_image_link }}">
                                            {%- endif -%}
                                            <a href="{{ block.settings.context_image_link }}"{% if block.settings.context_target_blank %} target="_blank"{% endif %} aria-label="{{ block.settings.context_image.alt | escape }}" class="{%- unless inside_link -%}section-block section-block--icon {%- endunless -%}" {% if settings.disable_right_click %} oncontextmenu="return false"{% endif %}>
                                        {%- else -%}
                                            <div class="section-block section-block--icon" {% if settings.disable_right_click %} oncontextmenu="return false"{% endif %}>
                                        {%- endif -%}
                                        <span class="icon-accordion--with-color" style="--color: {{ color_rgb }}; --mobile-color: {{ mobile_color_rgb }};">{% render 'catalog-icons', file_name: file_name, icon: icon %}</span>
                                        {%- if block.settings.context_image_link != blank -%}
                                            </a>{%- if inside_link -%}</object>{%- endif -%}  
                                        {%- else -%}
                                            </div>
                                        {%- endif -%}
                                    {%- endif -%}
                                    {%- if block.settings.heading != blank -%}
                                        {%- liquid
                                            assign uppercase_heading = false
                                            if block.settings.heading_style == 'caption-font' and settings.uppercase_caption
                                                assign uppercase_heading = true
                                            elsif block.settings.heading_style == 'body-font' and settings.uppercase_body
                                                assign uppercase_heading = true
                                            elsif block.settings.heading_style == 'subheading-font' and settings.uppercase_subheadings
                                                assign uppercase_heading = true
                                            elsif block.settings.heading_style == 'secondary-heading' and settings.uppercase_secondary_headings
                                                assign uppercase_heading = true
                                            elsif block.settings.heading_style == 'section-heading' and settings.uppercase_section_headings
                                                assign uppercase_heading = true
                                            elsif block.settings.heading_style == 'page-title' and settings.uppercase_page_titles
                                                assign uppercase_heading = true
                                            endif

                                            assign heading_text = block.settings.heading
                                            assign processed_text = ''
                                            assign split_text = heading_text | split: '['

                                            for part in split_text
                                            if part contains ']'
                                                assign split_bracket = part | split: ']'
                                                assign highlight_text = split_bracket[0]
                                                assign after_bracket = split_bracket[1]
                                                assign processed_text = processed_text | append: '<span class="highlight"><span>' | append: highlight_text | append: '</span></span>' | append: after_bracket
                                            else
                                                assign processed_text = processed_text | append: part
                                            endif
                                            endfor

                                            assign final_text = ''
                                            assign split_text_curly = processed_text | split: '{'

                                            for part in split_text_curly
                                            if part contains '}'
                                                assign split_curly = part | split: '}'
                                                assign styled_text = split_curly[0]
                                                assign after_curly = split_curly[1]
                                                assign final_text = final_text | append: '<span class="styled">' | append: styled_text | append: '</span>' | append: after_curly
                                            else
                                                assign final_text = final_text | append: part
                                            endif
                                            endfor
                                        -%}
