                                                    {% if settings.bolder_secondary_buttons_font %}<b>{% endif %}{{ block.settings.button_label_1 }}{% if settings.bolder_secondary_buttons_font %}</b>{% endif %}
                                                {% endif %}
                                            {%- if block.settings.button_link_1 != blank -%}</a>{% if inside_link %}</object>{% endif %}{%- else -%}</span>{%- endif -%}
