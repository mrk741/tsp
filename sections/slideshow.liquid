                                                <a href="{{ block.settings.button_link_3 }}"{% if block.settings.target_blank_3 %} target="_blank"{% endif %} class="button {{ block.settings.button_style_3 }}-button body-font {% if block.settings.use_main_button %} button--main{% if settings.use_heading_font_main_buttons %} heading-font-style{% endif %}{% if settings.uppercase_main_buttons %} uppercase{% endif %}{% else %} button--secondary{% if settings.use_heading_font_secondary_buttons %} heading-font-style{% endif %}{% if settings.uppercase_secondary_buttons %} uppercase{% endif %}{% endif %}{% if block.settings.button_icon_3 != blank %} button-with-icon{% if block.settings.swap_button_icon_3 %} icon--swap{% endif %}{% endif %}">
                                            {%- else -%}
                                                <span disabled class="button {{ block.settings.button_style_3 }}-button body-font{% if block.settings.use_main_button %} button--main{% if settings.use_heading_font_main_buttons %} heading-font-style{% endif %}{% if settings.uppercase_main_buttons %} uppercase{% endif %}{% else %} button--secondary{% if settings.use_heading_font_secondary_buttons %} heading-font-style{% endif %}{% if settings.uppercase_secondary_buttons %} uppercase{% endif %}{% endif %}{% if block.settings.button_icon_3 != blank %} button-with-icon{% if block.settings.swap_button_icon_3 %} icon--swap{% endif %}{% endif %}">
                                            {%- endif -%}
                                            {% if block.settings.button_icon_3 != blank %}
                                                {%- liquid
                                                  assign icon_name = block.settings.button_icon_3 | split: '/'
                                                  if icon_name.size == 3
                                                      assign file_name = icon_name.first | append: '_' | append: icon_name[1]
                                                  elsif icon_name.size == 2
                                                      assign file_name = icon_name.first
                                                  endif
                                                  assign icon = icon_name.last
                                                -%}
                                                <span class="icon-accordion flex--row {% if block.settings.use_main_button %}icon--large{% else %}icon--small{% endif %}">{% render 'catalog-icons', file_name: file_name, icon: icon %}</span>
                                              {% endif %}
                                                {% if block.settings.use_main_button %}
                                                    {% if settings.bolder_main_buttons_font %}<b>{% endif %}{{ block.settings.button_label_3 }}{% if settings.bolder_main_buttons_font %}</b>{% endif %}
                                                {% else %}
                                                    {% if settings.bolder_secondary_buttons_font %}<b>{% endif %}{{ block.settings.button_label_3 }}{% if settings.bolder_secondary_buttons_font %}</b>{% endif %}
                                                {% endif %}
                                            {%- if block.settings.button_link_3 != blank -%}</a>{% if inside_link %}</object>{% endif %}{%- else -%}</span>{%- endif -%}
                                        </div>
                                    {%- endif -%}
                                </div>
                            </div>
                            </div>
                        {% if block.settings.link != blank %}</a>{% else %}</div>{% endif %}
                {% endcase %}
            {%- endfor -%}
        </div>
        <div onselectstart="return false" class="swiper-button-prev swiper-button swiper-button-prev-{{ section.id }}{% if section.settings.show_arrows == false %} visually-hidden{% endif %}"><span class="icon icon--small">{%- render 'glyphs_arrows', icon: 'arrow-left' -%}</span></div>
